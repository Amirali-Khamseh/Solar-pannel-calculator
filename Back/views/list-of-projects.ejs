<!doctype html>
<html lang="en" data-bs-theme="auto">


<%- include('./partials/head') %>

    <body>
        <%- include('./partials/dashboard-nav') %>
            <div class="container ">
                <div class="row col-12">
                    <table class="table  table-dark border border-success ">
                        <thead>
                            <tr>

                                <th scope="col">Project's name</th>
                                <th scope="col">Project's description</th>
                                <th scope="col">Project's Owner</th>
                                <th scope="col">Project's Details</th>
                                <th scope="col">Delete a project</th>
                                <th scope="col">Edit project</th>


                            </tr>
                        </thead>
                        <tbody>
                            <% projects.forEach((project)=>{ %>
                                <tr>
                                    <td class="table-light">
                                        <%= project.title %>
                                    </td>
                                    <td class="table-info">
                                        <%= project.description %>
                                    </td>
                                    <td class="table-warning">
                                        <%= project.createdBy.name %>
                                    </td>
                                    <td class="table-primary">

                                        <a class="link-dark" href="/projects/update/<%= project._id %>">Edit üîÑ</a>
                                    </td>
                                    <td class="table-danger ">
                                        <a class="link-dark" href="/projects/delete/<%= project._id %>">Delete ‚ùå</a>


                                    </td>
                                    <td class="table-secondary">
                                        <a class="link-dark" href="/projects/details/<%= project._id %>">View üßê</a>
                                    </td>
                                </tr>
                                <% }) %>
                        </tbody>
                    </table>
                </div>
            </div>

            <%- include('./partials/copyright') %>
    </body>


</html>